{
  "name": "PureJSMVC",
  "version": "1.0.0",
  "description": "A simple user register app using Webpack for bundle and server, Jest for testing, Less and ES6/ES7/ES8.",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --mode development --open",
    "build": "webpack --mode production",
    "test": "jest --verbose"
  },
  "author": "Celso Henrique",
  "devDependencies": {
    "autoprefixer": "^9.1.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.4.0",
    "eslint-loader": "^2.1.0",
    "foundation-sites": "^6.5.0-rc.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.5.0",
    "node-sass": "^4.9.3",
    "nunjucks": "^3.1.3",
    "nunjucks-loader": "^3.0.0",
    "postcss-loader": "^3.0.0",
    "regenerator-runtime": "^0.12.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.22.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|sass)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "url-loader": "^1.1.1"
  }
}
